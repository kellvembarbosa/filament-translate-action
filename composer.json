{
    "name": "pxlrbt/filament-translate-action",
    "description": "Translate Action for Filament Admin",
    "license": "MIT",
    "keywords": [
        "laravel",
        "filament",
        "laravel-filament",
        "translate"
    ],
    "authors": [
        {
            "name": "Dennis Koch",
            "email": "info@pixelarbeit.de"
        }
    ],
    "require": {
        "php": "^8.0",
        "filament/filament": "^3.0.0"
    },
    "require-dev": {
        "laravel/pint": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "pxlrbt\\FilamentTranslateAction\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "pxlrbt\\FilamentTranslateAction\\TranslateActionServiceProvider"
            ]
        }
    },
    "scripts": {
        "pint": "vendor/bin/pint"
    }
}
